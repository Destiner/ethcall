{
  "name": "ethcall",
  "version": "6.0.5",
  "description": "ethers.js-compatible wrapper around Multicall",
  "author": "Timur Badretdinov <destinerx@gmail.com>",
  "keywords": [
    "ethers",
    "ethereum"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Destiner/ethcall.git"
  },
  "type": "module",
