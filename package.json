    "eslint-plugin-import": "2.29.1",
    "prettier": "3.2.4",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vitest": "1.1.1"
  },
  "peerDependencies": {
    "ethers": "^6.0.0"
